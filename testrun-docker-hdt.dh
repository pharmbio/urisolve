#!/bin/bash
export HDTFILE=example_dataset.hdt
echo "After the service started below, open a browser and surf in on http://localhost:8080/entity/Q3 ..."
docker run \
    -it \
    -p 8080:8080 \
    --name urisolve-test-hdt \
    --hostname urisolve \
    --rm farmbio/urisolve \
    urisolve \
    -srctype hdt \
    -hdtfile $HDTFILE \
    -urihost http://rdf.pharmb.io \
    -host urisolve \
    -port 8080
